{
    "name": "goodt-wcore",
    "version": "0.1.13",
    "private": true,
    "main": "src/core/index.js",
    "types": "src/core/index.d.ts",
    "scripts": {
        "dev": "vite",
        "serve": "vue-cli-service serve",
        "build": "npm run lint && npm run format && npm run build:lib",
        "build:lib": "vue-cli-service build --target lib --dest ./dist/ --name goodt-wcore ./src/core/index.js",
        "lint": "eslint src/",
        "lint:fix:essential": "putout src/ --fix",
        "lint:style": "stylelint 'src/**/*.{css,scss,less,pcss,vue}'",
        "lint:all": "scripts/lint/eslint-all.sh",
        "lint:all:fix": "npm run lint -- --fix",
        "lint:staged": "scripts/lint/eslint-staged.sh",
        "lint:staged:short": "scripts/lint/eslint-staged-short.sh",
        "lint:committed": "scripts/lint/eslint-committed.sh",
        "lint:committed:short": "scripts/lint/eslint-committed-short.sh",
        "format": "prettier --write ./src/**/*",
        "test:unit": "VUE_CLI_CONTEXT=./tests vue-cli-service test:unit",
        "docs:panel-ui": "npx vue-styleguidist server --config ./styleguide.config.js",
        "docs:panel-ui:build": "vue-styleguidist build --config ./styleguide.config.js",
        "widget:create": "npx goodt-scaffold",
        "widget:panel": "npx goodt-scaffold-panel"
    },
    "dependencies": {
        "@goodt-common/api": "file:src/common/services/api",
        "@goodt-common/domain": "file:src/common/domain",
        "@goodt-common/errors": "file:src/common/errors",
        "@goodt-common/infra": "file:src/common/infra",
        "@goodt-common/mixins": "file:src/common/mixins",
        "@goodt-common/services": "file:src/common/services",
        "@goodt-common/utils": "file:src/common/utils",
        "@goodt-wcore/common": "file:src/common",
        "@goodt-wcore/core": "file:src/core",
        "@goodt-wcore/managers": "file:src/core/managers",
        "@goodt-wcore/mixins": "file:src/core/mixins",
        "@goodt-wcore/net": "file:src/core/net",
        "@goodt-wcore/panels": "file:src/core/panels",
        "@goodt-wcore/render": "file:src/core/render",
        "@goodt-wcore/scaffold": "file:scripts/scaffold",
        "@goodt-wcore/styleguide": "file:scripts/styleguide",
        "@goodt-wcore/types": "file:src/core/types",
        "@goodt/browserslist-config": "file:.config/browserslist-config",
        "@goodt/config": "file:.config",
        "@goodt/eslint-config-base": "file:.config/eslint-config-base",
        "@goodt/stylelint-config-base": "file:.config/stylelint-config-base",
        "@goodt/tests": "file:tests",
        "axios": "^0.21.1",
        "goodt-dremio-sdk": "^1.0.21",
        "inflection": "^1.13.1",
        "keycloak-js": "^10.0.0",
        "lodash": "^4.17.21",
        "portal-vue": "^2.1.7",
        "uuid": "^8.3.2",
        "vuedraggable": "^2.24.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.5.13",
        "@vue/cli-service": "^4.5.13",
        "friendly-errors-plugin": "^1.1.2",
        "goodt-framework-css": "^1.1.2",
        "less": "^4.1.1",
        "less-loader": "^6.0.0",
        "npm-commands": "^1.1.0",
        "prettier": "git+ssh://git@github.com:mayoujin/prettierx.git",
        "vite": "^2.4.1",
        "vite-plugin-vue2": "^1.7.3",
        "webpack": "^4.46.0",
        "yorkie": "^2.0.0"
    },
    "peerDependencies": {
        "goodteditor-ui": "^0.0.34",
        "vue": "^2.6.12"
    },
    "engines": {
        "node": ">=14",
        "npm": ">=7"
    }
}
