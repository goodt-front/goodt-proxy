{
    "name": "goodt-wcore",
    "version": "0.1.0",
    "private": true,
    "main": "src/core/index.js",
    "types": "src/core/index.d.ts",
    "scripts": {
        "dev": "vite",
        "serve": "vue-cli-service serve",
        "build": "npm run lint && npm run format && npm run build:lib",
        "build:lib": "vue-cli-service build --target lib --dest ./dist/ --name goodt-wcore ./src/core/index.js",
        "lint": "eslint src/",
        "lint:all": "scripts/lint/eslint-all.sh",
        "lint:all:fix": "npm run lint -- --fix",
        "lint:staged": "scripts/lint/eslint-staged.sh",
        "lint:staged:short": "scripts/lint/eslint-staged-short.sh",
        "lint:committed": "scripts/lint/eslint-committed.sh",
        "lint:committed:short": "scripts/lint/eslint-committed-short.sh",
        "format": "prettier --write ./src/**/*",
        "test:unit": "VUE_CLI_CONTEXT=./tests vue-cli-service test:unit",
        "docs:panel-ui": "npx vue-styleguidist server --config ./styleguide.config.js",
        "docs:panel-ui:build": "vue-styleguidist build --config ./styleguide.config.js",
        "scaffold": "npx goodt-scaffold",
        "scaffold:panel": "npx goodt-scaffold-panel",
        "prepare": "npm run dev-run -- dev-run_husky-install",
        "dev-run": "node ./scripts/run-local.js",
        "dev-run_husky-install": "npx husky install"
    },
    "dependencies": {
        "@goodt/browserslist-config": "file:.config/browserslist-config",
        "@goodt/common": "file:src/common",
        "@goodt/config": "file:.config",
        "@goodt/core": "file:src/core",
        "@goodt/eslint-config-base": "file:scripts/lint",
        "@goodt/panels": "file:src/core/panels",
        "@goodt/scaffold": "file:scripts/scaffold",
        "@goodt/styleguide": "file:scripts/styleguide",
        "@goodt/stylelint-config-base": "file:scripts/lint/stylelint-config-base",
        "@goodt/tests": "file:tests",
        "axios": "^0.21.1",
        "goodt-dremio-sdk": "^1.0.21",
        "inflection": "^1.13.1",
        "keycloak-js": "^10.0.0",
        "lodash": "^4.17.21",
        "portal-vue": "^2.1.7",
        "uuid": "^8.3.2",
        "vuedraggable": "^2.24.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.5.13",
        "@vue/cli-service": "^4.5.13",
        "goodt-framework-css": "^1.1.2",
        "less": "^4.1.1",
        "less-loader": "^6.0.0",
        "npm-commands": "^1.1.0",
        "vite": "^2.4.1",
        "vite-plugin-vue2": "^1.7.3",
        "webpack": "^4.46.0"
    },
    "peerDependencies": {
        "goodteditor-ui": "^0.0.32",
        "vue": "^2.6.12"
    },
    "lint-staged": {
        "*.(js|jsx|vue|ts)": [
            "npm run lint"
        ]
    },
    "engines": {
        "node": ">=14",
        "npm": ">=7"
    }
}
